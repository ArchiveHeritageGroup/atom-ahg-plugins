{
    "name": "Digital Asset Management",
    "machine_name": "ahgDAMPlugin",
    "version": "1.3.9",
    "description": "Digital Asset Management with IPTC metadata, watermarks, derivatives, and Creative Commons licensing",
    "author": "The Archive and Heritage Group",
    "license": "GPL-3.0",
    "category": "ahg",
    "requires": {
        "atom_framework": ">=1.0.0",
        "atom": ">=2.8",
        "php": ">=8.1"
    },
    "install_sql": "data/install.sql",
    "tables": [
        "dam_iptc_metadata",
        "media_derivatives",
        "watermark_type",
        "watermark_setting",
        "custom_watermark",
        "object_watermark_setting",
        "creative_commons_license",
        "creative_commons_license_i18n",
        "rights_derivative_rule",
        "rights_derivative_log",
        "security_watermark_log"
    ],
    "theme_support": [
        "arDominionB5Plugin",
        "ahgThemeB5Plugin"
    ]
}
