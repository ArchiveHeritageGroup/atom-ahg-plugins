{
    "name": "Field Mapping Designer",
    "machine_name": "ahgMigrationPlugin",
    "version": "1.0.3",
    "description": "[DEPRECATED - Use ahgDataMigrationPlugin instead] Field mapping designer for Vernon CMS, ArchivesSpace, DB/TextWorks imports.",
    "deprecated": true,
    "replacement": "ahgDataMigrationPlugin",
    "author": "The Archive and Heritage Group",
    "license": "GPL-3.0",
    "category": "migration",
    "requires": {
        "atom_framework": ">=1.0.0",
        "atom": ">=2.8",
        "php": ">=8.1"
    },
    "dependencies": [
        "ahgCorePlugin"
    ],
    "tables": [
        "atom_migration_job",
        "atom_migration_template",
        "atom_migration_log",
        "atom_migration_staged"
    ],
    "settings": {
        "migration_upload_path": "uploads/migration",
        "migration_max_file_size": 104857600,
        "migration_batch_size": 100,
        "migration_default_output": "direct",
        "migration_create_actors": true,
        "migration_create_terms": true
    }
}
