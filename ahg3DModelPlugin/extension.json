{
    "name": "3D Model Viewer",
    "machine_name": "ahg3DModelPlugin",
    "version": "1.0.0",
    "description": "3D model viewing with Google Model Viewer, AR support, hotspots, and IIIF 3D manifests",
    "author": "The Archive and Heritage Group",
    "license": "GPL-3.0",
    "category": "ahg",
    "requires": {
        "atom_framework": ">=1.0.0",
        "atom": ">=2.8",
        "php": ">=8.1"
    },
    "install_sql": "data/install.sql",
    "tables": [
        "object_3d_model",
        "object_3d_model_i18n",
        "object_3d_hotspot",
        "object_3d_hotspot_i18n",
        "object_3d_texture",
        "object_3d_settings",
        "viewer_3d_settings",
        "iiif_3d_manifest",
        "object_3d_audit_log"
    ],
    "theme_support": ["arDominionB5Plugin", "ahgThemeB5Plugin"]
}
