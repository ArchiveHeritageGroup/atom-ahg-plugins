    "author": "The Archive and Heritage Group",
    "license": "GPL-3.0",
    "category": "compliance",
    "requires": {
        "atom_framework": ">=1.0.0",
        "atom": ">=2.8",
        "php": ">=8.1"
    },
    "install_sql": "data/install.sql",
    "tables": [
        "grap_heritage_asset",
        "grap_valuation_history",
        "grap_impairment_assessment",
