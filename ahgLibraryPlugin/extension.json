{
    "name": "Library & Bibliographic",
    "machine_name": "ahgLibraryPlugin",
    "version": "1.9.14",
    "description": "Library cataloging with MARC-inspired fields, ISBN lookup, and bibliographic management",
    "author": "The Archive and Heritage Group",
    "license": "GPL-3.0",
    "category": "sector",
    "requires": {
        "atom_framework": ">=1.0.0",
        "atom": ">=2.8",
        "php": ">=8.1"
    },
    "dependencies": [
        "ahgCorePlugin"
    ],
    "install_sql": "database/install.sql",
    "tables": [
        "library_item",
        "library_item_creator",
        "library_item_subject",
        "atom_isbn_cache",
        "atom_isbn_lookup_audit",
        "atom_isbn_provider",
        "library_settings"
    ]
}
